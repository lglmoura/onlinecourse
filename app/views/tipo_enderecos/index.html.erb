<h1>Listing tipo_enderecos</h1>

<table>
  <tr>
    <th>Descricao</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @tipo_enderecos.each do |tipo_endereco| %>
  <tr>
    <td><%= tipo_endereco.descricao %></td>
    <td><%= link_to 'Show', tipo_endereco %></td>
    <td><%= link_to 'Edit', edit_tipo_endereco_path(tipo_endereco) %></td>
    <td><%= link_to 'Excluir', tipo_endereco, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>


<h1>Listing tipo_telefones</h1>

<table>
  <tr>
    <th>Descricao</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @tipo_telefones.each do |tipo_telefone| %>
  <tr>
    <td><%= tipo_telefone.descricao %></td>
    <td><%= link_to 'Show', tipo_telefone %></td>
    <td><%= link_to 'Edit', edit_tipo_telefone_path(tipo_telefone) %></td>
    <td><%= link_to 'Excluir', tipo_telefone, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Tipo telefone', new_tipo_telefone_path %>
